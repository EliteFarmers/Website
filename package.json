{
	"name": "elitedev",
	"version": "1.0.0",
	"type": "module",
	"engines": {
		"node": ">=24.0.0"
	},
	"packageManager": "pnpm@10.15.0",
	"scripts": {
		"dev": "concurrently \"pnpm:dev:content\" \"pnpm:dev:svelte\"",
		"dev:farming-weight": "pnpm -C packages/farming-weight build:watch",
		"dev:all": "concurrently \"pnpm:dev:content\" \"pnpm:dev:svelte\" \"pnpm:dev:farming-weight\"",
		"dev:content": "velite dev --watch",
		"dev:svelte": "pnpm vite dev",
		"devhost": "vite dev --host",
		"build": "pnpm build:content && pnpm build:farming-weight && pnpm build:svelte",
		"build:content": "velite",
		"build:svelte": "vite build",
		"build:farming-weight": "pnpm -C packages/farming-weight build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"deploy": "node build/index.js",
		"check": "pnpm -C packages/farming-weight build && pnpm build:content && pnpm prepare && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint --max-warnings=0 .",
		"up": "docker-compose up -d database cache",
		"down": "docker-compose down",
		"license": "pnpm exec pnpm-licenses generate-disclaimer --output-file=./static/oss.txt",
		"generate-api": "pnpm orval",
		"postprocess-api": "node ./src/lib/api/util/morph-api.ts ./src/lib/api/client/EliteAPI.ts && pnpm prettier --write ./src/lib/api/client/EliteAPI.ts ./src/lib/api/schemas",
		"postprocess-zod": "node ./src/lib/api/util/morph-api.ts ./src/lib/api/client/EliteAPI.zod.ts && pnpm prettier --write ./src/lib/api/client/EliteAPI.zod.ts",
		"postprocess-cms": "node ./src/lib/api/util/morph-api.ts ./src/lib/api/client/EliteCms.zod.ts && pnpm prettier --write ./src/lib/api/client/EliteCms.zod.ts"
	},
	"dependencies": {
		"@napi-rs/canvas": "^0.1.81",
		"@sentry/sveltekit": "^10.25.0",
		"farming-weight": "workspace:*",
		"isomorphic-dompurify": "^2.31.0",
		"vanilla-cookieconsent": "^3.1.0"
	},
	"devDependencies": {
		"@date-fns/utc": "^2.1.1",
		"@eslint/js": "^9.39.0",
		"@exodus/schemasafe": "^1.3.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.542.0",
		"@number-flow/svelte": "^0.3.9",
		"@orval/zod": "^7.16.0",
		"@popperjs/core": "^2.11.8",
		"@quantco/pnpm-licenses": "^2.2.1",
		"@sinclair/typebox": "^0.34.41",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "2.48.4",
		"@sveltejs/vite-plugin-svelte": "6.2.0",
		"@tailwindcss/vite": "^4.1.17",
		"@tanstack/table-core": "^8.21.3",
		"@types/d3-array": "^3.2.2",
		"@types/d3-format": "^3.0.4",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-time-format": "^4.0.3",
		"@types/eslint": "^9.6.1",
		"@types/node": "^24.10.0",
		"@types/qs": "^6.14.0",
		"@typeschema/class-validator": "^0.2.0",
		"@vinejs/vine": "^1.8.0",
		"arktype": "^2.1.25",
		"bits-ui": "2.14.3",
		"class-validator": "^0.14.2",
		"classnames": "^2.5.1",
		"clsx": "^2.1.1",
		"concurrently": "^9.2.1",
		"d3-array": "^3.2.4",
		"d3-format": "^3.1.0",
		"d3-quadtree": "^3.0.1",
		"d3-scale": "^4.0.2",
		"d3-time-format": "^4.1.0",
		"date-fns": "^4.1.0",
		"dotenv": "^17.2.3",
		"embla-carousel-svelte": "8.6.0",
		"embla-carousel-wheel-gestures": "^8.1.0",
		"emoji-picker-element": "^1.27.0",
		"eslint": "^9.39.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"formsnap": "2.0.1",
		"globals": "^16.5.0",
		"joi": "^17.13.3",
		"layercake": "^10.0.2",
		"layerchart": "2.0.0-next.42",
		"marked": "^16.4.2",
		"mdsx": "^0.0.7",
		"mode-watcher": "^1.1.0",
		"orval": "^7.16.0",
		"paneforge": "1.0.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"qs": "^6.14.0",
		"rehype-github-alerts": "^4.1.1",
		"rehype-pretty-code": "^0.14.1",
		"rehype-slug": "^6.0.0",
		"remark-gfm": "^4.0.1",
		"runed": "^0.36.0",
		"superstruct": "^2.0.2",
		"svelte": "^5.43.6",
		"svelte-check": "^4.3.3",
		"svelte-dnd-action": "^0.9.67",
		"svelte-sonner": "^1.0.5",
		"svelte-time": "^2.0.2",
		"sveltekit-rate-limiter": "^0.7.0",
		"sveltekit-superforms": "^2.28.0",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.17",
		"ts-morph": "^27.0.2",
		"tslib": "^2.8.1",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.4",
		"valibot": "^1.1.0",
		"vaul-svelte": "1.0.0-next.7",
		"velite": "^0.3.0",
		"vite": "^7.2.2",
		"yup": "^1.7.1",
		"zod": "^4.1.12"
	}
}