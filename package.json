		"typescript": "^5.0.4",
		"vite": "^4.3.8"
	},
	"type": "module",
	"dependencies": {
		"@popperjs/core": "^2.11.7",
		"classnames": "^2.3.2",
		"flowbite": "^1.6.5",
		"flowbite-svelte": "^0.36.2",
		"openapi-fetch": "^0.6.1",
		"pg": "^8.7.3",
		"prismarine-nbt": "^2.2.1",
		"sequelize": "^6.21.3"
	}
