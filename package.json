{
	"name": "elitedev",
	"version": "1.0.0",
	"type": "module",
	"engines": {
		"node": ">=24.0.0"
	},
	"packageManager": "pnpm@10.15.0",
	"scripts": {
		"dev": "concurrently \"pnpm:dev:content\" \"pnpm:dev:svelte\"",
		"dev:content": "velite dev --watch",
		"dev:svelte": "pnpm vite dev",
		"devhost": "vite dev --host",
		"build": "pnpm build:content && pnpm build:svelte",
		"build:content": "velite",
		"build:svelte": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"deploy": "node build/index.js",
		"check": "pnpm build:content && pnpm prepare && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint --max-warnings=0 .",
		"up": "docker-compose up -d database cache",
		"down": "docker-compose down",
		"license": "pnpm exec pnpm-licenses generate-disclaimer --output-file=./static/oss.txt",
		"generate-api": "pnpm orval",
		"postprocess-api": "node ./src/lib/api/util/morph-api.ts ./src/lib/api/client/EliteAPI.ts && pnpm prettier --write ./src/lib/api/"
	},
	"dependencies": {
		"@napi-rs/canvas": "^0.1.76",
		"isomorphic-dompurify": "^2.26.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@exodus/schemasafe": "^1.3.0",
		"@internationalized/date": "^3.8.2",
		"@lucide/svelte": "^0.542.0",
		"@number-flow/svelte": "^0.3.9",
		"@orval/zod": "^7.11.2",
		"@popperjs/core": "^2.11.8",
		"@quantco/pnpm-licenses": "^2.2.1",
		"@sinclair/typebox": "^0.34.38",
		"@sveltejs/adapter-node": "^5.3.2",
		"@sveltejs/kit": "2.42.1",
		"@sveltejs/vite-plugin-svelte": "6.2.0",
		"@tailwindcss/vite": "^4.1.12",
		"@tanstack/table-core": "^8.21.3",
		"@types/d3-array": "^3.2.1",
		"@types/d3-format": "^3.0.4",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-time-format": "^4.0.3",
		"@types/eslint": "^9.6.1",
		"@types/node": "^24.3.0",
		"@typeschema/class-validator": "^0.2.0",
		"@vinejs/vine": "^1.8.0",
		"arktype": "^2.1.20",
		"bits-ui": "2.9.6",
		"class-validator": "^0.14.2",
		"classnames": "^2.5.1",
		"clsx": "^2.1.1",
		"concurrently": "^9.2.1",
		"d3-array": "^3.2.4",
		"d3-format": "^3.1.0",
		"d3-quadtree": "^3.0.1",
		"d3-scale": "^4.0.2",
		"d3-time-format": "^4.1.0",
		"date-fns": "^4.1.0",
		"dotenv": "^17.2.1",
		"embla-carousel-svelte": "8.6.0",
		"embla-carousel-wheel-gestures": "^8.0.2",
		"emoji-picker-element": "^1.26.3",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.1",
		"farming-weight": "^0.10.6",
		"formsnap": "2.0.1",
		"globals": "^16.3.0",
		"joi": "^17.13.3",
		"layercake": "^10.0.1",
		"layerchart": "^2.0.0-next.37",
		"marked": "^16.2.1",
		"mdsx": "^0.0.7",
		"mode-watcher": "^1.1.0",
		"orval": "^7.11.2",
		"paneforge": "1.0.0-next.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"rehype-pretty-code": "^0.14.1",
