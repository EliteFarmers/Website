{
	"name": "elitedev",
	"version": "1.0.0",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"devhost": "vite dev --host",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"deploy": "node build/index.js",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .eslintignore --check --plugin-search-dir=. . && eslint --max-warnings=0 .",
		"format": "prettier --ignore-path .eslintignore --write --plugin-search-dir=. .",
		"up": "docker-compose up -d database cache",
		"down": "docker-compose down",
		"generate-api-types": "npx openapi-typescript \"./src/lib/api/swagger.json\" -o \"./src/lib/api/api.d.ts\""
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^4.0.0",
		"@sveltejs/kit": "^2.4.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "10.4.7",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-config-standard-with-typescript": "^34.0.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite-svelte": "^0.40.2",
		"flowbite-svelte-icons": "^0.2.6",
		"postcss": "^8.4.33",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-preprocess": "^5.1.3",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.11"
	},
	"type": "module",
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"apexcharts": "^3.45.1",
		"classnames": "^2.5.1",
		"farming-weight": "^0.3.4",
		"flowbite": "^1.8.1",
		"openapi-fetch": "^0.7.10",
		"prismarine-nbt": "^2.5.0"
	}
}
