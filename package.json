		"@types/d3-format": "^3.0.4",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-time-format": "^4.0.3",
		"@types/eslint": "^9.6.1",
		"@typeschema/class-validator": "^0.2.0",
		"@vinejs/vine": "^1.8.0",
		"apexcharts": "^3.54.1",
