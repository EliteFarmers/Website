{
	"name": "elitedev",
	"version": "1.0.0",
	"type": "module",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"devhost": "vite dev --host",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"deploy": "node build/index.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --ignore-path .eslintignore --write .",
		"lint": "prettier --ignore-path .eslintignore --check . && eslint --max-warnings=0 .",
		"up": "docker-compose up -d database cache",
		"down": "docker-compose down",
		"license": "pnpm exec pnpm-licenses generate-disclaimer --output-file=./static/oss.txt",
		"generate-api-types": "npx openapi-typescript \"./src/lib/api/swagger.json\" -o \"./src/lib/api/api.d.ts\""
	},
	"devDependencies": {
		"@internationalized/date": "^3.6.0",
		"@popperjs/core": "^2.11.8",
		"@quantco/pnpm-licenses": "^2.1.0",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.8.0",
		"@sveltejs/vite-plugin-svelte": "4.0.2",
		"@types/d3-array": "^3.2.1",
		"@types/d3-format": "^3.0.4",
		"@types/d3-scale": "^4.0.8",
		"@types/d3-time-format": "^4.0.3",
		"@types/eslint": "^9.6.1",
		"apexcharts": "^3.54.1",
		"autoprefixer": "10.4.20",
		"bits-ui": "1.0.0-next.64",
		"classnames": "^2.5.1",
		"clsx": "^2.1.1",
		"d3-array": "^3.2.4",
		"d3-format": "^3.1.0",
		"d3-quadtree": "^3.0.1",
		"d3-scale": "^4.0.2",
		"d3-time-format": "^4.1.0",
		"date-fns": "^4.1.0",
		"embla-carousel-svelte": "8.5.1",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"farming-weight": "^0.7.0",
		"globals": "^15.12.0",
		"layercake": "^8.4.2",
		"layerchart": "^0.59.4",
		"lucide-svelte": "^0.461.0",
		"mode-watcher": "^0.5.0",
		"openapi-fetch": "^0.13.0",
		"postcss": "^8.4.48",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"svelte": "^5.1.13",
		"svelte-check": "^4.0.6",
		"svelte-preprocess": "^6.0.3",
		"svelte-sonner": "^0.3.28",
		"svelte-ux": "^0.75.4",
		"sveltekit-rate-limiter": "^0.6.1",
		"sveltekit-superforms": "^2.20.0",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.14",
