{
	"name": "farming-weight",
	"version": "0.12.2",
	"description": "Tools for calculating farming weight and fortune in Hypixel Skyblock",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc --watch",
		"format": "pnpm biome check --write",
		"lint": "pnpm biome check",
		"test": "vitest run",
		"test:watch": "vitest",
		"debug": "vitest --test-timeout=0 --no-file-parallelism"
	},
	"keywords": [
		"farming",
		"weight",
		"farming-weight",
		"skyblock",
		"hypixel",
		"hypixel-skyblock",
		"fortune"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/EliteFarmers/FarmingWeight.git"
	},
	"files": [
		"dist/**/*"
	],
	"bugs": {
		"url": "https://github.com/EliteFarmers/FarmingWeight/issues"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"homepage": "https://github.com/EliteFarmers/FarmingWeight#readme",
	"author": "Kaeso",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@types/madge": "^5.0.3",
		"madge": "^8.0.0",
		"typescript": "^5.8.3",
		"vitest": "^4.0.16"
	},
	"packageManager": "pnpm@9.15.0"
}